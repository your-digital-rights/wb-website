{
  "name": "whiteboar-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:css": "stylelint '**/*.css' --config context/design-system/stylelint.config.cjs",
    "test": "jest src/__tests__/components src/__tests__/unit",
    "test:unit": "jest src/__tests__/components src/__tests__/unit",
    "test:ci": "jest src/__tests__/components src/__tests__/unit",
    "test:ci:local": "./scripts/test-ci-local.sh",
    "test:integration": "PORT=3783 next dev --turbopack & sleep 5 && jest src/__tests__/integration; kill %1",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:onboarding": "playwright test onboarding.spec.ts",
    "test:e2e:onboarding:performance": "playwright test onboarding-performance.spec.ts",
    "test:e2e:onboarding:accessibility": "playwright test onboarding-accessibility.spec.ts",
    "test:e2e:onboarding:advanced": "playwright test onboarding-advanced.spec.ts",
    "test:e2e:onboarding:all": "playwright test onboarding*.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "db:push": "supabase db push --db-url \"$POSTGRES_URL_NON_POOLING\"",
    "db:push:dry": "supabase db push --db-url \"$POSTGRES_URL_NON_POOLING\" --dry-run",
    "db:migration:list": "supabase migration list --db-url \"$POSTGRES_URL_NON_POOLING\""
  },
